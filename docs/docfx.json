{
  "metadata": [
    {
      "src": [
        {
          "src": "../Assets/Plugins/PsychoUnity/Runtime",
          "files": [
            "**/*.cs"
          ]
        }
      ],
      "dest": "api",
      "outputFormat": "apiPage",
      "allowCompilationErrors": true,
      "memberLayout": "separatePages"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "sitemap": {
      "baseUrl": "https://psychounity.org",
      "priority": 1.0,
      "changefreq": "daily"
    },
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "PsychoUnity",
      "_appTitle": "PsychoUnity",
      "_appLogoPath": "images/PsychoUnityLogo.svg",
      "_appFaviconPath": "images/PsychoUnityLogoFavicon.svg",
      "_enableSearch": true,
      "pdf": true
    }
  }
}
